// Класс Женщина
class Woman extends Human {
    private int age;

    public Woman(String name, int age) {
        super(name);
        this.age = age;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    @Override
    public void displayInfo() {
        System.out.println("Женщина: " + getName() + ", Возраст: " + age);
    }
}
